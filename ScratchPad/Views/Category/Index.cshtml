@model List<ScratchPad.Models.Category>
@{
    ViewBag.Title = "Index";
}

<h2>Categories</h2>
<table class="table">
    <thead>
    <tr>
        <th>
            CategoryID
        </th>
        <th>
            Category Name
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var cat in Model)
    {
        <tr>
            <td>@cat.CategoryID</td>
            <td>@cat.CategoryName</td>
        </tr>
    }

    </tbody>
</table>
